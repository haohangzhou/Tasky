{
  "name": "group-web-project-group-af",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node build-api/server.js",
    "watch": "concurrently 'npm:watch:api' 'npm:watch:web' -c green.bold,magenta.bold",
    "watch:api": "nodemon",
    "watch:web": "react-scripts start",
    "build": "concurrently 'npm:build:api' 'npm:build:web' -c green.bold,magenta.bold",
    "build:api": "tsc -p tsconfig.api.json",
    "build:web": "react-scripts build",
    "test": "npm run test:web",
    "test:web": "react-scripts test",
    "lint": "eslint src/",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/morgan": "^1.9.3",
    "@types/node": "^12.20.24",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": "14.x"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
