{
  "name": "tasky",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "scripts": {
    "start": "node build-api/api/server.js",
    "start:api": "ts-node -P tsconfig.api.json src/api/server.ts",
    "watch": "concurrently 'npm:watch:api' 'npm:watch:web' -c green.bold,magenta.bold",
    "watch:api": "ts-node-dev --rs --respawn -P tsconfig.api.json src/api/server.ts",
    "watch:web": "react-scripts start",
    "build": "concurrently 'npm:build:api' 'npm:build:web' -c green.bold,magenta.bold",
    "build:api": "tsc -p tsconfig.api.json",
    "build:web": "react-scripts build",
    "clean": "del-cli build build-api coverage",
    "test": "npm run test:api && npm run test:web",
    "test:api": "jest --verbose",
    "test:web": "react-scripts test --testPathIgnorePatterns=/api/ --watchAll=false",
    "test:web:watch": "react-scripts test --testPathIgnorePatterns=/api/",
    "coverage:api": "npm run test:api -- --coverage",
    "coverage:web": "npm run test:web -- --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier src/ --write",
    "format:check": "prettier src/ --check",
    "db:seed": "ts-node -P tsconfig.api.json src/api/seed-database.ts",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@date-io/date-fns": "^2.11.0",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.5",
    "@mui/lab": "5.0.0-alpha.53",
    "@mui/material": "^5.0.6",
    "@mui/x-data-grid": "^5.0.0-beta.6",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "formik": "^2.2.9",
    "jdenticon": "^3.1.1",
    "jwt-promisify": "^1.1.3",
    "material-ui-popup-state": "^2.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.8.0",
    "sequelize-typescript": "^2.1.1",
    "use-local-storage-state": "^11.0.0",
    "web-vitals": "^1.1.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.2",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.6.6",
    "concurrently": "^6.3.0",
    "del-cli": "^4.0.1",
    "jest": "26.6.0",
    "prettier": "^2.4.1",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": "16.x",
    "npm": ">=7.x"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
